{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.sap.project4",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.19.3",
      "toolsId": "69c30f75-b9a9-42ae-b326-6ce4baadf369"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.141.3",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.sap.project4.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.f.routing.Router",
        "transition": "slide",
        "type": "View",
        "controlId": "pollution",
        "viewType": "XML",
        "path": "com.sap.project4.view",
        "async": true,
        "viewPath": "com.sap.project4.view"
      },
      "routes": [
        {
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master"
					]
				},
				{
					"pattern": "detail/{country}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
        },
        {
					"pattern": "detail/{country}/state/{state}/{layout}",
					"name": "city",
					"target": [
						"master",
						"detail",
						"city"
					]
				}
      ],
      "targets": {
        "master": {
					"viewName": "Master",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"viewName": "Detail",
					"controlAggregation": "midColumnPages"
        },
				"city": {
					"viewName": "City",
					"controlAggregation": "endColumnPages"
				}
      }
    },
    "rootView": {
      "viewName": "com.sap.project4.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
